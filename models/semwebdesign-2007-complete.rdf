        </swrc_ext:authorList>
        <owl:sameAs rdf:resource="http://revyu.com/things/semwebdesign-2007-paper-509-drift-a"/>
        <maker rdf:resource="http://data.semanticweb.org/person/kikuo-fujita"/>
