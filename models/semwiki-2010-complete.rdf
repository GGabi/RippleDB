        <maker rdf:resource="http://data.semanticweb.org/person/iadh-ounis"/>
        <maker rdf:resource="http://data.semanticweb.org/person/norman-gray"/>
        <maker rdf:resource="http://data.semanticweb.org/person/stuart-chalmers"/>
    </swrc:InProceedings>

    <rdf:Description rdf:about="http://data.semanticweb.org/workshop/semwiki/2010/paper/main/6/authorlist">
