    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/paper/learning-to-re-rank-query-dependent-image-re-ranki">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
