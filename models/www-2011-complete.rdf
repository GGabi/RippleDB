    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/paper/finding-hierarchy-in-directed-online-social-networ">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
