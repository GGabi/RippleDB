    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/paper/counting-triangles-and-the-curse-of-the-last-reduc">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
