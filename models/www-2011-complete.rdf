    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/paper/consideration-set-generation-in-commerce-search">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
