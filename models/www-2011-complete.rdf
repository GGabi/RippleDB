    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/phd/fuzzy-associative-rule-based-approach-for-pattern-">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
