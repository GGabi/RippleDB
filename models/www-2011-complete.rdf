    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/paper/incentivizing-high-quality-user-generated-content">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
