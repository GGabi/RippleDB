    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/phd/addressing-the-rdfa-publishing-bottleneck">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
