    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/paper/finding-the-bias-and-prestige-of-nodes-in-networks">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
