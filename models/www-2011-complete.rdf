    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/paper/a-self-training-approach-for-resolving-object-core">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
