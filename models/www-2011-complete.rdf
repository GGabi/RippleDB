    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/paper/learning-to-rank-with-multiple-objective-functions">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
