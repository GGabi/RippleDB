    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/paper/context-sensitive-query-auto-completion">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
