    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/www/2011/paper/a-game-theoretic-formulation-of-the-service-provis">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/www/2011/proceedings"/>
