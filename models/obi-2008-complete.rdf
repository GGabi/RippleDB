    <swrc:InProceedings rdf:about="http://data.semanticweb.org/workshop/obi/2008/paper/main/6">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/workshop/obi/2008/proceedings"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/jan-nemrava"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/jan-rauch"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/jiri-splichal"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/marek-nekvasil"/>
