    <swrc:InProceedings rdf:about="http://data.semanticweb.org/workshop/terra_cognita/2008/paper/main/8">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/workshop/terra_cognita/2008/proceedings"/>
