   <rdfs:label>The Talia library platform - Rapidly building a digital library on Rails</rdfs:label>
   <foaf:maker rdf:resource="http://data.semanticweb.org/person/daniel-hahn"/>
   <foaf:maker rdf:resource="http://data.semanticweb.org/person/michele-barbera"/>
