        <dc:creator rdf:resource="http://data.semanticweb.org/person/dafydd-gibbon"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/jolanta-bachan"/>
        <dc:subject>Corpus (creation, annotation, etc.)</dc:subject>
