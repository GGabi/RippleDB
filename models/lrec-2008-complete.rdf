        <swrc:url rdf:resource="http://www.lrec-conf.org/proceedings/lrec2008/slides/7.pdf"/>
        <swrc:url rdf:resource="http://www.lrec-conf.org/proceedings/lrec2008/summaries/7.html"/>
        <swrc:year>2008</swrc:year>
        <rdfs:label>A General Methodology for Mapping EuroWordNets to the Suggested Upper Merged Ontology</rdfs:label>
