        <swrc:url rdf:resource="http://www.lrec-conf.org/proceedings/lrec2008/summaries/471.html"/>
        <swrc:year>2008</swrc:year>
        <rdfs:label>Mapping Events and Abstract Entities from PAROLE-SIMPLE-CLIPS to ItalWordNet</rdfs:label>
