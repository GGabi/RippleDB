        <swrc:url rdf:resource="http://www.lrec-conf.org/proceedings/lrec2008/summaries/739.html"/>
        <swrc:year>2008</swrc:year>
        <rdfs:label>Treebank-Based Acquisition of LFG Parsing Resources for French</rdfs:label>
