        <swrc:url rdf:resource="http://www.lrec-conf.org/proceedings/lrec2008/slides/691.pdf"/>
        <swrc:url rdf:resource="http://www.lrec-conf.org/proceedings/lrec2008/summaries/691.html"/>
        <swrc:year>2008</swrc:year>
