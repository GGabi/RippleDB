        <dc:creator rdf:resource="http://data.semanticweb.org/person/erin-fitzgerald"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/frederick-jelinek"/>
        <dc:subject>Corpus (creation, annotation, etc.)</dc:subject>
        <dc:subject>Dialogue &#38; Natural Interactivity</dc:subject>
