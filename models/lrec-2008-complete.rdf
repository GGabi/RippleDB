    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/lrec/2008/papers/149">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/lrec/2008/proceedings"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/dietmar-roesner"/>
