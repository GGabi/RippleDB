    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/lrec/2008/papers/102">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/lrec/2008/proceedings"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/doroteo-t-toledano"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/javier-garrido"/>
