        <swrc:year>2008</swrc:year>
        <rdfs:label>STC-TIMIT: Generation of a Single-channel Telephone Corpus</rdfs:label>
        <maker rdf:resource="http://data.semanticweb.org/person/doroteo-t-toledano"/>
