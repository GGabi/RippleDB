    <swrc:InProceedings rdf:about="http://data.semanticweb.org/conference/lrec/2008/papers/484">
        <swc:isPartOf rdf:resource="http://data.semanticweb.org/conference/lrec/2008/proceedings"/>
        <dc:creator rdf:resource="http://data.semanticweb.org/person/joerg-tiedemann"/>
        <dc:subject>Corpus (creation, annotation, etc.)</dc:subject>
