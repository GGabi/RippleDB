        <swrc:url rdf:resource="http://www.lrec-conf.org/proceedings/lrec2008/summaries/875.html"/>
        <swrc:year>2008</swrc:year>
        <rdfs:label>A Hybrid Morphology-Based POS Tagger for Persian</rdfs:label>
