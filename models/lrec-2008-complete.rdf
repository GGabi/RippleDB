        <swrc:url rdf:resource="http://www.lrec-conf.org/proceedings/lrec2008/summaries/84.html"/>
        <swrc:year>2008</swrc:year>
        <rdfs:label>A Conceptual Approach to Web Image Retrieval</rdfs:label>
