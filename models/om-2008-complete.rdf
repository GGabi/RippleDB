        <swrc:year>2008</swrc:year>
        <swrc_ext:authorList rdf:parseType="Resource">
            <rdf:_1 rdf:resource="http://data.semanticweb.org/person/stefan-dietze"/>
            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/john-domingue"/>
        </swrc_ext:authorList>
        <rdfs:label>Towards ontology interoperability through conceptual groundings</rdfs:label>
        <maker rdf:resource="http://data.semanticweb.org/person/john-domingue"/>
