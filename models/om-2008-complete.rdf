            <rdf:_2 rdf:resource="http://data.semanticweb.org/person/jorge-gracia"/>
        </swrc_ext:authorList>
        <rdfs:label>Spider: bringing non-equivalence mappings to OAEI</rdfs:label>
        <maker rdf:resource="http://data.semanticweb.org/person/jorge-gracia"/>
        <maker rdf:resource="http://data.semanticweb.org/person/marta-sabou"/>
